            <JFXButton fx:id="btnRoles" onAction="#onActionButtonRoles" prefHeight="32.0" prefWidth="350.0" text="Roles" />
            <JFXButton fx:id="btnCerrarSesión" onAction="#onActionButtonCerrarSesión" prefHeight="32.0" prefWidth="350.0" styleClass="buttonSalir" text="Cerrar sesión" />
         </children>
      </VBox>
   </children>

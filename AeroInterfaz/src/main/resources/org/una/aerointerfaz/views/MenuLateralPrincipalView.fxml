<?xml version="1.0" encoding="UTF-8"?>

<?import com.jfoenix.controls.JFXButton?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="439.0" prefWidth="349.0" stylesheets="@../resources/estilo.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="org.una.aerointerfaz.controllers.MenuLateralPrincipalViewController">
   <children>
      <StackPane alignment="TOP_CENTER" prefHeight="128.0" prefWidth="340.0">
         <children>
            <ImageView fitHeight="136.0" fitWidth="355.0" pickOnBounds="true">
               <image>
                  <Image url="@../resources/UNAeropuerto.jpg" />
               </image></ImageView>
         </children>
      </StackPane>
      <VBox alignment="CENTER" prefHeight="300.0" prefWidth="329.0" spacing="5.0">
